@nko_letter      = [anko      eenko      inko      enko      unko      oonko      onko      dagbasinnanko];
@nko_letter_init = [anko.init eenko.init inko.init enko.init unko.init oonko.init onko.init dagbasinnanko.init];
@nko_letter_medi = [anko.medi eenko.medi inko.medi enko.medi unko.medi oonko.medi onko.medi dagbasinnanko.medi];
@nko_letter_fina = [anko.fina eenko.fina inko.fina enko.fina unko.fina oonko.fina onko.fina dagbasinnanko.fina];
feature init {
	script nko;
	lookupflag RightToLeft;
	sub @nko_letter by @nko_letter_init;
} init;
feature medi {
	script nko;
	lookupflag RightToLeft;
	sub @nko_letter by @nko_letter_medi;
} medi;
feature fina {
	script nko;
	lookupflag RightToLeft;
	sub @nko_letter by @nko_letter_fina;
} fina;

