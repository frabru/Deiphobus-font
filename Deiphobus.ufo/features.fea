} bhookvar;

feature locl {
	script latn;
	language TRK;
	lookup idot {
		sub i by Idotaccent;
	} idot;
	language AZE;
	lookup idot;
	language CRT;
	lookup idot;

	language SMI; lookup engvar;
	language SME; lookup engvar;
