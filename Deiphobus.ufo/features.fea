lookup alternatives {
	sub florin from [Fhook florin.sign];
	sub Eng from [Eng Eng.sami];
} alternatives;

feature calt {
	ignore substitute florin' [@letter.latn @combining_diacritic];
	ignore substitute [@letter.latn @combining_diacritic] florin';
	sub @digit florin' by florin.sign;
	sub @digit @whitespace florin' by florin.sign;
	sub florin' @digit by florin.sign;
	sub florin' @whitespace @digit by florin.sign;
	sub florin' @whitespace [plus hyphen] @digit by florin.sign;
} calt;

feature aalt {
	lookup alternatives;
} aalt;
