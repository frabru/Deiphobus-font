	pos base [J] <anchor 858 1408> mark @TOP_MARKS;
	pos base [Jecyrillic] <anchor 1045 1408> mark @TOP_MARKS;
	pos base [L Lslash Lcaron Lcedilla] <anchor 550 1408> mark @TOP_MARKS;
	pos base [Ljecyrillic] <anchor 671 1408> mark @TOP_MARKS;
	pos base [Tsecyrillic] <anchor 671 1408> mark @TOP_MARKS;
