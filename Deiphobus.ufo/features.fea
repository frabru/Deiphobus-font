lookup engvar {
	sub Eng by Eng.sami;
} engvar;

feature locl {
	script latn;
	language TRK;
	lookup idot {
		sub i by Idotaccent;
	} idot;
	language AZE;
	lookup idot;
	language CRT;
	lookup idot;

	language SMI; lookup engvar;
	language SME; lookup engvar;
	language SJE; lookup engvar;
	language SJU; lookup engvar;
	language SMJ; lookup engvar;
	language SMS; lookup engvar;
	language SJD; lookup engvar;
	language SJK; lookup engvar;
	language SMA; lookup engvar;
	language SMN; lookup engvar;
} locl;

